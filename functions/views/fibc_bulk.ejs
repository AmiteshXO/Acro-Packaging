    <%include header1%>
    <!-- Mobile Specific Meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Favicon-->
    <link rel="shortcut icon" href="img/fav.png">
    <!-- Author Meta -->
    <meta name="author" content="Acro Packaging">
    <!-- Meta Description -->
    <meta name="description" content="If you are looking for FIBC Bulk Bags, Acro Packaging is one of the leading packaging company to meet all your packing demands">
    <!-- Meta Keyword -->
    <meta name="keywords" content="Bulk Bags, FIBC Bulk Bags,FIBC Bulk Bags prices, Acro Packaging, FIBC Bulk Bags for sale, FIBC Bulk Bags manufacturer, FIBC Bulk Bags  uses, FIBC Bulk Bags india, FIBC Bulk Bags specification">
    <!-- meta character set -->
    <meta charset="UTF-8">
    <!-- Site Title -->
    <title>FIBC Bulk Bags | Acro Packaging</title>
    
    <%include header2%>
    <!-- start banner Area -->
    <section class="banner-area relative about-banner" style="background: url(../img/ie.jpg)" id="home"> 
      <div class="overlay overlay-bg"></div>
      <div class="container">       
        <div class="row d-flex align-items-center justify-content-center">
          <div class="about-content col-lg-12">
            <h1 class="text-white">
              FIBC Bulk Bags
            </h1> 
            <p class="text-white link-nav"><a href="index#scroll2">Products </a>  <span class="lnr lnr-arrow-right"></span>  <a href="#"> FIBC Bulk Bags</a></p>
          </div>
        </div>
      </div>
    </section>
    <!-- End banner Area -->  
    
    <!-- Start Products Area -->
    <section class="products-area section-gap" id ="scroll2">
      <div class="container">
        <div class="row">
          <% for(let i=1;i<15;i++) { %>
          <div class="col-lg-4">
            <div class="single-latest-blog">
              <div class="thumb">
                <img class="img-fluid" src="img/products/p<%=i%>.jpg" alt="<%=data[i].name%>">
              </div>
              <br>
              <a href="product?id=<%=i%>">
                <h4><%=data[i].name%></h4>
              </a>
              <p>
                <%=data[i].desc.substring(0,96)%>. .
              </p>
              <a href="product?id=<%=i%>" class="genric-btn info circle">More Info</a>
            </div>
          </div>
          <% } %>

        </div>
      </div>  
    </section>
    <!-- End products Area -->


    <% include footer%>